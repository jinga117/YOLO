<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
     "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
    <mapper namespace="com.kosta.yolo.dao.TripInfoMapper">
    
    	<select id="selectInfoAll" resultType="com.kosta.yolo.vo.TripInfoVO">
    		SELECT * FROM TripInfo
    	</select>
    	<select id="selectListAge" resultType="com.kosta.yolo.vo.TripInfoVO">
    		SELECT * FROM TripInfo where age_id like '%a1%'
    	</select>
    	
    </mapper>